{
  "version": 2,
  "name": "maplenou-api",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "5000"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}
